define inline_script inc_counter
	set description "Executes Python code"
	___run__
	exp.set("counter", exp.get("counter")+1)
	__end__
	set _prepare ""

define inline_script incorrect_trigger
	set description "Executes Python code"
	___run__
	global io
	if self.get("response") == self.get("cresp"):
		trigger = 220
	else:
		trigger = 221
	port = 0x1FF0
	try:
		io.DlPortWritePortUchar(port, trigger)
	except:
		print 'failed to send trigger'
	__end__
	set _prepare ""

define sketchpad instructions_1
	set duration keypress
	set description "Displays stimuli"
	draw textline center=1 color=white font_bold=no font_family=mono font_italic=no font_size=18 html=yes show_if=always text="Welcome! <br /><br />In this experiment, you will be shown words in the center of the screen. <br /><br />Your task is to decide whether the word shown is a *real word* or a *nonreal word.* <br /><br />After the word has displayed, you should respond by pressing the TOP button on the joystick for a REAL word, <br /><br />or the BOTTOM button for a NONREAL word. <br /><br />After every three words, there will be a symbol displayed on the screen that looks like this: <br /><br />(--) <br /><br />which indicates that this is an optimal time for you to blink. <br /><br />If that is not displaying, please try your best to avoid blinking or moving around. <br /><br />After each block of 75 words, you will have self-timed breaks to rest. In total, there will be 4 blocks. At the end, please wait for the experimenter to come in. <br /><br />Please wave or call out to the experimenters during this time if there is anything you need. <br /><br />Thank you! <br /><br />" x=0 y=0 z_index=0

define logger logger
	set description "Logs experimental data"
	set auto_log yes
	log target
	log targ_type
	log cresp
	log condtrigtime

define inline_script logscript
	set description "Executes Python code"
	___run__
	log.write_vars(var.condtrigtime)
	log.write_vars(var.wordtrigtime)
	log.write_vars(var.canvastime)
	__end__
	set _prepare ""

define coroutines new_coroutines
	set function_name ""
	set flush_keyboard yes
	set duration 1000
	set description "Run items simultaneously"
	run target_present end=0 runif=always start=0
	run new_keyboard_response_1 end=1000 runif=always start=0
	run responsescreen end=395 runif=always start=395

define inline_script new_inline_script
	set description "Executes Python code"
	set _run ""
	set _prepare ""

define keyboard_response new_keyboard_response_1
	set timeout 1000
	set flush yes
	set duration keypress
	set description "Collects keyboard responses"
	set correct_response "[cresp]"
	set allowed_responses "z;/"

define inline_script reset_counter_prep
	set description "Executes Python code"
	set _run ""
	___prepare__
	exp.set('counter',0)
	__end__


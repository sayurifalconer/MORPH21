set width 1920
set uniform_coordinates yes
set title "morph21_Oliver/Morris"
set subject_parity even
set subject_nr 0
set start AB_m21
set sound_sample_size -16
set sound_freq 48000
set sound_channels 2
set sound_buf_size 1024
set sampler_backend legacy
set round_decimals 2
set mouse_backend xpyriment
set keyboard_backend legacy
set height 1080
set fullscreen no
set form_clicks no
set foreground white
set font_underline no
set font_size 18
set font_italic no
set font_family mono
set font_bold no
set experiment_path "/Volumes/hiplab/MORPH_21/final_expts"
set disable_garbage_collection yes
set description "The main experiment item"
set coordinates uniform
set compensation 0
set color_backend legacy
set clock_backend legacy
set canvas_backend xpyriment
set bidi no
set background black

define sequence AB_m21
	set flush_keyboard yes
	set description "Runs a number of items in sequence"
	run start_trigger always
	run reset_counter_prep always
	run reset_counter_run always
	run instructions_1 always
	run block1 always
	run end_screen always

define sketchpad ITI
	set duration "[ITI]"
	set description "Displays stimuli"

define sketchpad blank_screen
	set duration 497
	set description "Displays stimuli"

define sketchpad blink_screen
	set duration 1500
	set description "Displays stimuli"
	draw textline center=1 color=white font_bold=no font_family=mono font_italic=no font_size=18 html=yes show_if=always text="(--)" x=0 y=0 z_index=0


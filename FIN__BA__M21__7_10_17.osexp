define inline_script canvas
	set description "Executes Python code"
	___run__
	def targcanvasfxn():
		targcanvas = canvas(autoprepare=False)
		targcanvas.text(var.target, center=True)
		targcanvas.color=u"white"
		targcanvas.background_color=u"black"
		targcanvas.font_family= u"mono"
		targcanvas.font_size=18
		targcanvas.prepare()
		targcanvas.show()
		global canvastime
		canvastime = time.time(targcanvas)		
		return canvastime
	__end__
	set _prepare ""

define inline_script correct_trigger
	set description "Executes Python code"
	___run__
	global io
	trigger = 220
	port = 0x1FF0
	try:
		io.DlPortWritePortUchar(port, trigger)
	except:
		print 'failed to send trigger' 
	__end__
	set _prepare ""

define inline_script def_chain
	set description "Executes Python code"
	___run__
	items["target_present"].run = chain(items["target_present"].run, cond_trigger())
	__end__
	___prepare__
	def chain(*functions): 
		return lambda: [f() for f in functions]
	
	def cond_trigger():
		global io
		trigger = self.get("cond_trig")
		port = 0x1FF0
		try:
			io.DlPortWritePortUchar(port, trigger)
		except:
			print 'failed to send trigger' 
		global condtrigtime
		condtrigtime =  self.time()
	__end__


	set description "Executes Python code"
	___run__
	exp.set('counter',0) 
	__end__
	set _prepare ""

define sketchpad responsescreen
	set duration 600
	set description "Displays stimuli"

define sequence sequence
	set flush_keyboard yes
	set description "Runs a number of items in sequence"
	run inc_counter always
	run fixation_1 always
	run blank_screen always
	run def_chain always
	run new_coroutines always
	run correct_trigger "[correct]=1"
	run incorrect_trigger always
	run blink_screen "[counter]%3=0"
	run ITI always
	run word_trigger always
	run break1 "[counter]=75"
	run break2 "[counter]=150"
	run break3_1 "[counter]=225"
	run logger always

define inline_script start_trigger
	set description "Executes Python code"
	set _run ""
	___prepare__
	try:
		from ctypes import windll
		global io 
		io= windll.dlportio
	except:
		print 'the parallel port could not be opened'
	__end__

